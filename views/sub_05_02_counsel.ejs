<script type="text/javascript" src="/javascripts/applicants.js"></script>

<script>
    const inspectDom = document.getElementsByName('inspect');

    inspectDom.forEach(dom => {
        dom.addEventListener('change', e => {
            if (dom.value == '신청안함') {
                inspectDom.forEach(dom => {
                    dom.checked = false;
                });
                dom.checked = true;
            } else {
                document.getElementById('inspect-null').checked = false;
            }
        });
    });

    $(document).ready(function () {
			$.datepicker.setDefaults($.datepicker.regional['ko']);

			$("#birth").datepicker({
				changeMonth: true,
				changeYear: true,
				nextText: '다음 달',
				prevText: '이전 달',
				dayNames: ['일요일', '월요일', '화요일', '수요일', '목요일', '금요일', '토요일'],
				dayNamesMin: ['일', '월', '화', '수', '목', '금', '토'],
				monthNamesShort: ['1월', '2월', '3월', '4월', '5월', '6월', '7월', '8월', '9월', '10월', '11월', '12월'],
				monthNames: ['1월', '2월', '3월', '4월', '5월', '6월', '7월', '8월', '9월', '10월', '11월', '12월'],
				dateFormat: "yymmdd",
				minDate: -40000,
				maxDate: 0, // 선택할수있는 최소날짜, ( 0 : 오늘 이후 날짜 선택 불가)
				yearRange: "-120:+120",
				showMonthAfterYear: true,
			});

			$("#inspect_date").datepicker({
				changeMonth: true,
				changeYear: true,
				nextText: '다음 달',
				prevText: '이전 달',
				dayNames: ['일요일', '월요일', '화요일', '수요일', '목요일', '금요일', '토요일'],
				dayNamesMin: ['일', '월', '화', '수', '목', '금', '토'],
				monthNamesShort: ['1월', '2월', '3월', '4월', '5월', '6월', '7월', '8월', '9월', '10월', '11월', '12월'],
				monthNames: ['1월', '2월', '3월', '4월', '5월', '6월', '7월', '8월', '9월', '10월', '11월', '12월'],
				dateFormat: "yymmdd",
				minDate: -10000,
				maxDate: 0, // 선택할수있는 최소날짜, ( 0 : 오늘 이후 날짜 선택 불가)
				yearRange: "-120:+1",
				showMonthAfterYear: true,
			});
    });
</script>
